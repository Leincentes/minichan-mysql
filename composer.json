{
    "name": "leinc/minichan-mysql",
    "description": "A minimal mysql framework.",
    "type": "lib",
    "keywords": ["library", "mysql", "database"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MinichanMysql\\": "lib/"
        }
    },
    "authors": [
        {
            "name": "Niel Vincent Cano",
            "email": "leincentes@gmail.com"
        }
    ],
    "suggest": {
        "ext-pdo_mysql": "Use MySQL or MariaDB database"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0",
        "ext-pdo": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.4"
    },
    "scripts": {
        "test": "vendor/phpunit/phpunit/phpunit tests"
    }
}
